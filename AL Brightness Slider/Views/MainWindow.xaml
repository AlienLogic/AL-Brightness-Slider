<Window x:Class="AL_Brightness_Slider.Views.MainWindow"
		  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
		  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
		  xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
		  xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
		  xmlns:views="clr-namespace:AL_Brightness_Slider.Views"
		  mc:Ignorable="d"
		  Height="34" Width="360"
		  WindowStyle="None"
		  Background="#FF1F1F1F">

	<Window.Resources>
		<ResourceDictionary>
			<ResourceDictionary.MergedDictionaries>
				<ResourceDictionary Source="..\Resources\Colors.xaml" />
			</ResourceDictionary.MergedDictionaries>
		</ResourceDictionary>
	</Window.Resources>

	<StackPanel Orientation="Vertical" Name="monitorsPanel">
		<Label FontFamily="Segoe UI" FontSize="11.5pt" HorizontalAlignment="Left" Padding="8pt">Luminosità</Label>
	</StackPanel>
</Window>
